{
    "name": "@wya/vc",
    "version": "2.0.0-alpha.3",
    "description": "wya for vue components",
    "main": "lib/index.js",
    "files": [
        "lib/**"
    ],
    "scripts": {
        "start": "npm run dev",
        "dev": "npm run prompt && npm run all",
        "serve": "node --max_old_space_size=8192 ./node_modules/webpack-dev-server/bin/webpack-dev-server --config  ./config/webpack.config.dev.js --color --progress --hot",
        "demo": "npm run remove:dist && cross-env UGLIFY_JS=true node --max_old_space_size=8192 ./node_modules/webpack/bin/webpack --config ./config/webpack.config.dist.js --color --progress",
        "build": "npm run test && rm -rf ./lib && cross-env NODE_ENV=production node sfc.js",
        "pub": "bash config/publish.sh",
        "remove:dist": "rm -rf ./dist",
        "lint": "esw config ./src --ext .vue,.js --color",
        "lint:fix": "npm run lint -- --fix",
        "lint:watch": "npm run lint -- --fix --watch",
        "lint:style": "stylelint --fix --syntax scss src/**/*.scss",
        "mock": "cd ./src/mock && npm run start",
        "prompt": "node ./config/prompt.config.js",
        "all": "npm-run-all --parallel --race --continue-on-error lint:watch serve",
        "lint-staged": "lint-staged",
        "pre-commit:install": "cp .hooks/* .git/hooks/",
        "readme": "node ./config/readme.config.js",
        "test:common": "node ./tests/prompt.config.js && cross-env BABEL_ENV=test karma start tests/karma.config.js",
        "test": "npm run test:common -- --single-run",
        "test:watch": "npm run test:common"
    },
    "author": "wya-team",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/wya-team/wya-vc"
    },
    "bugs": {
        "url": "https://github.com/wya-team/wya-vc/issues"
    },
    "dependencies": {
        "async-validator": "~1.10.0",
        "echarts": "^4.2.0-rc.2",
        "html2canvas": "^1.0.0-alpha.12",
        "lodash": "^4.17.11",
        "mobile-drag-drop": "^2.3.0-rc.1",
        "normalize-wheel": "^1.0.1",
        "normalize.css": "^8.0.1",
        "photoswipe": "^4.1.2",
        "quill": "^1.3.6",
        "resize-detector": "^0.1.9",
        "resize-observer-polyfill": "^1.5.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.7.7",
        "@babel/core": "^7.7.7",
        "@babel/plugin-proposal-class-properties": "^7.7.4",
        "@babel/plugin-proposal-decorators": "^7.7.4",
        "@babel/plugin-proposal-export-default-from": "^7.7.4",
        "@babel/plugin-proposal-export-namespace-from": "^7.7.4",
        "@babel/plugin-proposal-function-bind": "^7.7.4",
        "@babel/plugin-syntax-dynamic-import": "^7.7.4",
        "@babel/plugin-syntax-jsx": "^7.7.4",
        "@babel/plugin-transform-block-scoping": "^7.7.4",
        "@babel/plugin-transform-runtime": "^7.7.6",
        "@babel/preset-env": "^7.7.7",
        "@babel/runtime": "^7.7.7",
        "@vue/test-utils": "^1.0.0-beta.30",
        "@wya/http": "^1.1.10",
        "@wya/ps": "^1.0.2",
        "@wya/utils": "^1.5.2",
        "autoprefixer": "^9.7.3",
        "babel-eslint": "^10.0.3",
        "babel-loader": "^8.0.6",
        "babel-plugin-import": "^1.13.0",
        "babel-plugin-istanbul": "^6.0.0",
        "babel-plugin-transform-vue-jsx": "^4.0.1",
        "chai": "^4.2.0",
        "copy-webpack-plugin": "^5.1.1",
        "cross-env": "^6.0.3",
        "css-loader": "^3.4.0",
        "cssnano": "^4.1.10",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-plugin-babel": "^5.3.0",
        "eslint-plugin-import": "^2.19.1",
        "eslint-plugin-markdown": "^1.0.1",
        "eslint-plugin-vue": "^6.0.1",
        "eslint-watch": "^6.0.1",
        "file-loader": "^5.0.2",
        "friendly-errors-webpack-plugin": "^1.7.0",
        "fs-extra": "^8.1.0",
        "glob": "^7.1.6",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^3.2.0",
        "inquirer": "^7.0.1",
        "inquirer-autocomplete-prompt": "^1.0.1",
        "karma": "^4.4.1",
        "karma-chrome-launcher": "^3.1.0",
        "karma-coverage": "^2.0.1",
        "karma-mocha": "^1.3.0",
        "karma-sinon-chai": "^2.0.2",
        "karma-sourcemap-loader": "^0.3.7",
        "karma-spec-reporter": "0.0.32",
        "karma-webpack": "^4.0.2",
        "lint-staged": "^9.5.0",
        "mocha": "^6.2.2",
        "mockjs": "^1.1.0",
        "node-sass": "^4.13.0",
        "npm-run-all": "^4.1.5",
        "postcss-flexbugs-fixes": "^4.1.0",
        "postcss-loader": "^3.0.0",
        "postcss-smart-import": "^0.7.6",
        "pre-commit": "^1.2.2",
        "progress-bar-webpack-plugin": "^1.12.1",
        "rimraf": "^3.0.0",
        "sass-loader": "^8.0.0",
        "sinon": "^8.0.1",
        "sinon-chai": "^3.4.0",
        "style-loader": "^1.1.1",
        "stylelint": "^12.0.0",
        "stylelint-config-standard": "^19.0.0",
        "stylelint-webpack-plugin": "^1.1.2",
        "upath": "^1.2.0",
        "url-loader": "^3.0.0",
        "vue": "^2.6.11",
        "vue-loader": "^15.8.3",
        "vue-style-loader": "^4.1.2",
        "vue-template-compiler": "^2.6.11",
        "webpack": "^4.41.4",
        "webpack-cli": "^3.3.10",
        "webpack-dev-server": "^3.10.1",
        "webpack-merge": "^4.2.2"
    },
    "peerDependencies": {
        "@babel/runtime": ">=7.0.0",
        "@wya/ps": ">=1.0.0",
        "@wya/http": ">=1.1.7",
        "@wya/utils": ">=1.5.1",
        "vue": ">=2.6.0"
    },
    "lint-staged": {
        "{src,config}/**/*.{js,jsx,vue}": [
            "esw --fix",
            "git add"
        ],
        "*.md": [
            "esw --ext '.md' --fix",
            "git add"
        ],
        "src/**/*.scss": [
            "stylelint --fix --syntax scss",
            "git add"
        ]
    },
    "pre-commit": [
        "lint-staged"
    ]
}
